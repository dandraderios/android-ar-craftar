<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.catchoom.com.test2">

<uses-sdk
    android:minSdkVersion="19"
    android:targetSdkVersion="22" />

<uses-permission android:name="android.permission.CAMERA" />
<uses-permission android:name="android.permission.CAMERA_EXTENDED" />
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
<uses-permission android:name="android.hardware.camera.autofocus" />

<application
    android:label="@string/app_name"
    android:icon="@mipmap/ic_launcher2"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
        android:name="test.catchoom.com.test2.SplashScreenActivity"
        android:label="@string/app_name"
        android:screenOrientation="portrait" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="test.catchoom.com.test2.ARProgrammaticallyActivity"
        android:screenOrientation="portrait" >
    </activity>
    <activity
        android:name="test.catchoom.com.test2.ARFromCraftARActivity"
        android:screenOrientation="portrait" >
    </activity>
    <activity
        android:name="test.catchoom.com.test2.RecognitionOnlyActivity"
        android:screenOrientation="portrait" >
    </activity>
    <activity
        android:name="test.catchoom.com.test2.OnDeviceARActivity"
        android:screenOrientation="portrait" >
    </activity>
    <activity
        android:name="test.catchoom.com.test2.LaunchersActivity"
        android:screenOrientation="portrait" >
    </activity>
    <activity
        android:name="test.catchoom.com.test2.HowToActivity"
        android:screenOrientation="portrait" >
    </activity>
    <activity
        android:name="test.catchoom.com.test2.WebActivity"
        android:screenOrientation="portrait" >
    </activity>
</application>

</manifest>